[project]
	description = Rudix is a collection of pre-built Unix software delivered as packages for Mac OS X.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/James Polley (jamezpolley)
[access "refs/for/refs/*"]
	read = group user/James Polley (jamezpolley)
	push = group user/James Polley (jamezpolley)
	label-Code-Review = -2..+2 group user/James Polley (jamezpolley)
	label-Verified = -1..+1 group user/James Polley (jamezpolley)
	rebase = group user/James Polley (jamezpolley)
	removeReviewer = group user/James Polley (jamezpolley)
	submit = group user/James Polley (jamezpolley)
[access "refs/heads/*"]
	read = group user/James Polley (jamezpolley)
	create = group user/James Polley (jamezpolley)
	push = +force group user/James Polley (jamezpolley)
	pushMerge = group user/James Polley (jamezpolley)
[access "refs/tags/*"]
	push = group user/James Polley (jamezpolley)
	push = +force group user/James Polley (jamezpolley)
